{% props
    cols = [],
    rows = [],
%}
<div {{  attributes }}>
    <table>
        <thead>
            <tr>
            {% for col in cols %}
                <th>{{ col }}</th>
            {% endfor %}
            </tr>
        </thead>
        <tbody>
        {% for row in rows %}
            <tr>
            {% for cell in row %}
                <td>{{ cell }}</td>
            {% endfor %}
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
