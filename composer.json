{
    "name": "sensiolabs/storybook-bundle",
    "type": "library",
    "license": "MIT",
    "description": "Bundle to use Storybook with Symfony UX packages.",
    "authors": [
        {
            "name": "Nicolas Rigaud",
            "email": "squrious@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Storybook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Storybook\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/console": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/twig-bundle": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "symfony/asset-mapper": "^7.0",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stimulus-bundle": "^2.16",
        "symfony/ux-live-component": "^2.16",
        "symfony/ux-twig-component": "2.16",
        "symfonycasts/sass-bundle": "^0.5.1",
        "symfonycasts/tailwind-bundle": "^0.5.0"
    },
    "conflict": {
        "symfony/ux-twig-component": "2.17"
    }
}
